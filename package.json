{
  "name": "hofstee",
  "version": "0.0.1",
  "description": "An app for a construction builder company",
  "productName": "hofstee",
  "author": "dan vincent tagailo",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:mobile": "cd src-cordova && cordova run android",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "remove:cordova": "quasar mode remove cordova",
    "add:cordova": "quasar mode add cordova",
    "add:platform": "cd src-cordova && cordova platform add android",
    "apk:build": "quasar build -m android -- -- --packageType=apk",
    "apk:zipalign": "zipalign -v 4 ./src-cordova/platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk Hofstee.apk",
    "apk:sign": "apksigner sign --ks my-release-key.keystore --ks-key-alias hofstee_app Hofstee.apk",
    "pkg:all": "npm run apk:build && npm run apk:zipalign && npm run apk:sign"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "firebase": "^10.12.2",
    "moment": "^2.30.1",
    "pinia": "^2.3.0",
    "pinia-plugin-persist": "^1.0.0",
    "quasar": "^2.6.0",
    "request": "^2.88.2",
    "vue": "^3.5.13",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.3.0",
    "@quasar/quasar-app-extension-dotenv": "^1.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.4.14",
    "prettier": "^3.0.0-alpha.6",
    "workbox-build": "7.0.x",
    "workbox-cacheable-response": "7.0.x",
    "workbox-core": "7.0.x",
    "workbox-expiration": "7.0.x",
    "workbox-precaching": "7.0.x",
    "workbox-routing": "7.0.x",
    "workbox-strategies": "7.0.x"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19 || 20.9",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
