{
  "name": "hofstee",
  "version": "0.0.1",
  "description": "An app for a construction builder company",
  "productName": "hofstee",
  "author": "dan vincent tagailo",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./src",
    "lint:fix": "eslint --ext .js,.ts,.vue ./src --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,scss,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,css,scss,html}\"",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:mobile": "cd src-cordova && cordova run android",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "build:capacitor": "quasar build -m capacitor -T android",
    "dev:capacitor": "quasar dev -m capacitor -T android",
    "remove:cordova": "quasar mode remove cordova",
    "add:cordova": "quasar mode add cordova",
    "add:platform": "cd src-cordova && cordova platform add android",
    "apk:build": "quasar build -m android -- -- --packageType=apk",
    "apk:zipalign": "rm -f Hofstee.apk && zipalign -v 4 ./src-cordova/platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk Hofstee.apk",
    "apk:sign": "apksigner sign --ks my-release-key.keystore --ks-key-alias hofstee_app Hofstee.apk",
    "pkg:all:old": "npm run apk:build && npm run apk:zipalign && npm run apk:sign",
    "apk:cap:build": "quasar build -m capacitor -T android",
    "apk:cap:zipalign": "rm -f Hofstee.apk && /Users/dhan/Library/Android/sdk/build-tools/30.0.3/zipalign -v 4 ./src-capacitor/android/app/build/outputs/apk/release/app-release-unsigned.apk Hofstee.apk",
    "apk:cap:sign": "/Users/dhan/Library/Android/sdk/build-tools/30.0.3/apksigner sign --ks my-release-key.keystore --ks-key-alias hofstee_app Hofstee.apk",
    "apk:pkg:all": "npm run build:capacitor && npm run apk:cap:zipalign && npm run apk:cap:sign"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/app": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/status-bar": "^6.0.3",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/timegrid": "^6.1.10",
    "@fullcalendar/vue3": "^6.1.10",
    "@hugotomazi/capacitor-navigation-bar": "^4.0.1",
    "@quasar/extras": "^1.16.4",
    "@quasar/quasar-ui-qcalendar": "^4.1.2",
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "firebase": "^10.12.2",
    "lucide-vue-next": "^0.525.0",
    "moment": "^2.30.1",
    "pinia": "^2.3.0",
    "pinia-plugin-persist": "^1.0.0",
    "quasar": "^2.18.1",
    "request": "^2.88.2",
    "vue": "^3.5.13",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.6",
